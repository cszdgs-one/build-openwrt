
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'ddns'
	option interface 'wan'
	option service_name '3322.org'
	option enabled '1'
	option lookup_host 'lcs999.3322.org'
	option domain 'lcs999.3322.org'
	option username 'lcs999'
	option password 'qwe321qwe'
	option ip_source 'web'

config service 'aliyun'
	option interface 'wan'
	option service_name 'aliyun.com'
	option enabled '1'
	option lookup_host 'x86.shunjiahong.com'
	option domain 'x86.shunjiahong.com'
	option username 'LTAI4FvKLeJGmi4Uumq8SKuc'
	option password 'gLc39oSJjhEE5QSB5ZBR9cN7JA6QQ9'
	option ip_source 'web'

config service 'myddns_ipv6'
	option use_ipv6 '1'
	option service_name 'example.org'
	option domain 'yourhost.example.org'
	option username 'your_username'
	option password 'your_password'
	option interface 'wan6'
	option ip_source 'network'
	option ip_network 'wan6'
	option lookup_host 'yourhost.example.org'

